{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rc-custom-blocks/homepage-hero",
  "version": "0.1.0",
  "title": "Homepage Hero",
  "category": "media",
  "icon": "admin-settings",
  "description": "Homepage hero section. Background overlay is tied to primary color picker in the customizer.",
  "attributes": {
    "backgroundImageArray": {
      "type": "array",
      "default": []
    },
    "frontImageUrl": {
      "type": "string"
    },
    "frontImageId": {
      "type": "integer"
    }
  },
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "dr-reuter",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}